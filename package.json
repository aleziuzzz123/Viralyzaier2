{
  "scripts": {
    "preinstall": "npx -y npm-force-resolutions || true",
    "postinstall": "node -e \"(async()=>{const m=await import('pixi.js');console.log('[pixi-version]',m.VERSION||(m.Application&&m.Application.VERSION)||'unknown')})().catch(e=>{console.error('[pixi-check-failed]',e?.message||e);process.exit(1)})\"",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@shotstack/shotstack-studio": "1.5.0",
    "pixi.js": "7.4.2"
  },
  "overrides": {
    "pixi.js": "7.4.2",
    "@pixi/*": "7.4.2"
  },
  "resolutions": {
    "pixi.js": "7.4.2",
    "@pixi/*": "7.4.2"
  },
  "engines": { "node": "20.x" },
  "packageManager": "npm@10"
}
