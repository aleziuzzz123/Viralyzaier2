{
  "version": 2,
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "cleanUrls": true,
  "trailingSlash": false,
  "routes": [
    { "src": "/studio", "dest": "/studio.html" },
    { "src": "/studio/", "dest": "/studio.html" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
